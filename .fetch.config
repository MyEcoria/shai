{
  "name": "example",
  "description": "Example agent with MCP fetch server for web scraping and API calls",
  "llm_provider": {
    "provider": "ovhcloud",
    "env_vars": {
      "OVH_BASE_URL": "https://qwen-3-32b.endpoints.kepler.ai.cloud.ovh.net/api/openai_compat/v1"
    },
    "model": "Qwen3-32B",
    "tool_method": "FunctionCall"
  },
  "tools": {
    "builtin": ["*"],
    "builtin_excluded": ["fetch"],
    "mcp": {
      "fetch": {
        "config": {
          "type": "stdio",
          "command": "uvx",
          "args": ["mcp-server-fetch"]
        },
        "enabled_tools": ["*"]
      }
    }
  },
  "temperature": 0.3
}
